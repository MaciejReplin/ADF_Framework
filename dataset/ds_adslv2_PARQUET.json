		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().ds_file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().ds_folder_name",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ds_container_name",
					"type": "Expression"
				}
			},
			"compressionCodec": "none"
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
