			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ds_container_name": {
				"type": "string"
			},
			"ds_folder_name": {
				"type": "string"
			},
			"ds_file_name": {
				"type": "string"
			}
		},
		"folder": {
