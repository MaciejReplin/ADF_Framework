{
	"name": "ds_adslv2",
	"properties": {
		"description": "Data set for Azure Data Lake Storage Gen 2 CSV files with container, folder and file name parameters.",
		"linkedServiceName": {
			"referenceName": "LS_AzureDataLakeStoragev2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"ds_container_name": {
				"type": "string"
			},
			"ds_folder_name": {
				"type": "string"
			},
			"ds_file_name": {
				"type": "string"
			}
		},
		"folder": {
			"name": "Datasets/ADSLv2/"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().ds_file_name",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().ds_folder_name",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().ds_container_name",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	},
	"type": "Microsoft.DataFactory/factories/datasets"
}